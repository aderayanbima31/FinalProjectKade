<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kade.derayanbimaalamsyah.finalprojectkade">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitleBar">
        <activity android:name=".view.baseuianko.activities.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.baseuianko.activities.detail.match.DetailMatchesActivity"
            android:parentActivityName=".view.baseuianko.activities.home.HomeActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.baseuianko.activities.detail.player.DetailPlayersActivity"
            android:parentActivityName=".view.baseuianko.activities.home.HomeActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.baseuianko.activities.detail.team.DetailTeamsActivity"
            android:parentActivityName=".view.baseuianko.activities.home.HomeActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>